    "@playform/compress": "^0.1.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "astro": "4.15.4",
    "astro-seo": "^0.8.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.78.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.10",
